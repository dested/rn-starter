{
  "name": "starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPhone X\"",
    "android:install": "adb install -r ./android/app/build/outputs/apk/release/app-release.apk ",
    "android:deploy": "npx jetify && cd android && gradlew assembleRelease && adb install -r ./app/build/outputs/apk/release/app-release.apk && cd ..",
    "android:deploy-ios": "npx jetify && cd android && ./gradlew assembleRelease && adb install -r ./app/build/outputs/apk/release/app-release.apk && cd ..",
    "android:bundle": "npx jetify && cd android && gradlew bundleRelease",
    "android:bundle-ios": "npx jetify && cd android && ./gradlew bundleRelease",
    "android:clean": "cd android && gradlew clean & cd .. & react-native run-android",
    "android:clean-ios": "cd android && ./gradlew clean & cd .. & react-native run-android",
    "tsc": "npx tsc",
    "pretty": "npx prettier \"*/**/*.ts*\"  --config .prettierrc --write",
    "lint": "npx tslint -p tsconfig.json --fix",
    "assets": "node scripts/process-images"
  },
  "dependencies": {
    "react": "16.9.0",
    "react-native": "0.61.5",

    "@react-native-community/async-storage": "1.8.1",
    "@react-native-community/datetimepicker": "^2.3.0",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/masked-view": "^0.1.7",
    "@react-native-community/netinfo": "^5.6.2",
    "@react-navigation/native": "^5.1.3",
    "@react-navigation/stack": "^5.2.6",
    "mobx": "^5.15.4",
    "mobx-persist": "^0.4.1",
    "mobx-react": "^6.1.8",
    "mobx-react-lite": "^1.5.2",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "react-native-animatable": "^1.3.3",
    "react-native-device-info": "^5.5.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/runtime": "^7.9.2",

    "@types/react-native-linear-gradient": "^2.4.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-transform-flow-strip-types": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/plugin-transform-typescript": "^7.9.0",
    "@types/react-native": "^0.61.23",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.9.0",

    "prettier": "^2.0.2",
    "ts-loader": "^6.2.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.2.0",
    "tslint-react": "^4.2.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
